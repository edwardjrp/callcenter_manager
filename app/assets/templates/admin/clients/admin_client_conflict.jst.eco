      <td><%=@present_client.last_name%></td>
      <td><input type='checkbox' class='overwrite_last_name'/></td>
    </tr>
    <tr>
      <td>CÃ©dula</td>
      <td><%=window.to_idnumber(@new_client.idnumber)%></td>
      <td><%=window.to_idnumber(@present_client.idnumber)%></td>
      <td><input type='checkbox' class='overwrite_idnumber'/></td>
    </tr>
    <tr>
      <td>Email</td>
      <td><%=@new_client.email%></td>
      <td><%=@present_client.email%></td>
      <td><input type='checkbox' class='overwrite_email'/></td>
    </tr>
    <tr>
      <td>Estado</td>
