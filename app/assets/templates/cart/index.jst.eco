<div class='panel'>
  <h3>Orden actual</h3>
  <div class='content'>
    <%if _.any(@model.get('cart_products').models):%>
      <ul>
        <%for cart_product in @model.get('cart_products').models:%>
          <li><%=cart_product.get('product').get('productname')%></li>
        <%end%>
      </ul>
    <%else:%>
      No hay productos en la orden
    <%end%>
  </div>
</div>