= render 'admin_products'
%h3.bottom-margin-1 Logs de importaci√≥n
.row
  .span3=button_to 'Importar productos', admin_import_logs_path(log_type: ImportLog.products_import), class: 'btn'
  .span3=button_to 'Importar cupones', admin_import_logs_path(log_type: ImportLog.coupons_import), class: 'btn'
  .span3
    =form_for :dgii, url: admin_taxpayer_identifications_path, html: {multipart: true, class: 'form-inline' } do |f|
      = f.file_field :datafile
      = submit_tag 'Cargar Numeros Fiscales', class: 'btn'

= will_paginate @import_logs
%table.table.table-striped.table-bordered.table-condensed
  %thead
    %tr
      %th Typo de tarea
      %th Estado
      %th Iniciada hace
      %th Iniciada en
      %td
  %tbody#import_logs_list
    = render 'import_logs_list', import_logs: @import_logs
= will_paginate @import_logs